{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "es2017",
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true
  }
}
